{
  "name": "livewire-devtools",
  "version": "1.0.0",
  "description": "devtools for Livewire!",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env PORT=8100 npm run dev:shell",
    "dev:shell": "cd shells/dev && webpack-dev-server --hot",
    "dev:chrome": "cd shells/chrome && webpack --watch",
    "lint": "eslint src --ext=js,vue && eslint shells/chrome/src && eslint shells/dev/src && eslint shells/electron/src",
    "build": "cd shells/chrome && cross-env NODE_ENV=production webpack --progress",
    "build:ff": "cd shells/firefox && cross-env NODE_ENV=production webpack --progress",
    "zip": "npm run zip:chrome && npm run zip:ff",
    "zip:chrome": "cd shells && zip -r -FS ../dist/livewire-devtools-$npm_package_version-chrome.zip chrome -x *src/* -x *webpack.config.js",
    "zip:ff": "web-ext build --filename livewire-devtools-$npm_package_version-firefox.zip -s shells/firefox -a dist -i src --overwrite-dest",
    "release": "node release.js && npm run build && npm run zip",
    "release:beta": "cross-env RELEASE_CHANNEL=beta npm run release"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rw4lll/livewire-devtools.git"
  },
  "keywords": [
    "livewire",
    "devtools",
    "laravel"
  ],
  "contributors": [
    "Marcel Pociot",
    "Sergei Shitikov"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rw4lll/livewire-devtools/issues"
  },
  "homepage": "https://github.com/rw4lll/livewire-devtools#readme",
  "dependencies": {
    "@vue/ui": "^0.5.1",
    "circular-json-es6": "^2.0.1",
    "lodash.debounce": "^4.0.8",
    "lodash.groupby": "^4.6.0",
    "vue": "^2.7.10",
    "vue-router": "^3",
    "vuex": "^3"
  },
  "devDependencies": {
    "buble": "^0.20.0",
    "buble-loader": "^0.5",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.7",
    "eslint": "^8.23.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.1",
    "eslint-plugin-vue": "^9.4.0",
    "file-loader": "^6.2",
    "friendly-errors-webpack-plugin": "^1.7",
    "inquirer": "^8.0",
    "launch-editor-middleware": "^2.6",
    "raw-loader": "^4.0",
    "semver": "^7.3",
    "start-server-and-test": "^1.14",
    "style-loader": "^2.0.0",
    "stylus": "^0.59",
    "stylus-loader": "^4.3.3",
    "terser-webpack-plugin": "^4.2.3",
    "url-loader": "^4",
    "vue-loader": "^15",
    "vue-template-compiler": "^2",
    "webpack": "^4",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4",
    "webpack-merge": "^4"
  },
  "engines": {
    "node": ">=16"
  }
}
